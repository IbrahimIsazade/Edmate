@model IEnumerable<FeatureDto>

<ul class="feature-container flex items-center">
    @if (Model.Count() == 0)
    {
        <p>No Features</p>
    }
    else {
        @foreach (var item in Model)
        {
            <li class="feature-item flex">
                <i class="fa-solid fa-check"></i>
                <span>@item.Title</span>
            </li>
        }
    }
</ul>
